{
  "name": "actions-cache",
  "license": "MIT",
  "main": "lib/restore.js",
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build -o dist/restore src/restore.ts && ncc build -o dist/save src/save.ts",
    "test": "jest",
    "all": "yarn build && yarn package"
  },
  "dependencies": {
    "@actions/cache": "^1.0.7",
    "@actions/core": "^1.2.3",
    "minio": "^7.0.18"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/minio": "^7.0.7",
    "@zeit/ncc": "^0.22.3",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.2",
    "typescript": "^4.3.2"
  }
}
